<% @groups.each do |group| %>
  <div class="panel panel-default">
       <b><%= label_tag "Group:"%> <%= label_tag group.name%></b>
      <% if current_user.id == group.owner_id %>
        <%= link_to "Edit", group_path(group.id)%>
      <%end%>
      </br>
    <div class="panel-body">
      <% group.users.each do |user| %>
       <%= label_tag user.username, nil, class:"label label-default" %>
      <% end %>

    </div>
  </div>
<% end %>